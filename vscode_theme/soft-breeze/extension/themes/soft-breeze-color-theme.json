    "semanticHighlighting": true,
    "semanticTokenColors": {
        // "module": {
        //     "foreground": "#D4D4D4",
        //     "fontStyle": "italic"
        // },
        "class": "#fdd481",
        "class.defaultLibrary": "#fdd481",
        "class.readonly": {
            "foreground": "#fdd481",
            "fontStyle": "bold"
        },
        "function": "#6699CC",
        "method": "#6699CC",
        "variable": "#D4D4D4",
        "variable.readonly": "#f19f73",
    },
    "tokenColors": [
        {
            "name": "Comments",
            "scope": "comment, punctuation.definition.comment",
            "settings": {
                "foreground": "#8c91a0",
                "fontStyle": "italic"
            }
        },
        {
            "name": "White",
            "scope": "source, text.log, constant.character.format.placeholder, meta.embedded.line, meta.function-call.arguments",
            "settings": {
                "foreground": "#D4D4D4"
            }
        },
        {
            "name": "Soft red",
            "scope": "variable.parameter.function.language.special.self, variable.language.special.self, variable.language.special.cls, variable.parameter.function.language.special.cls, variable.language.this, entity.name.tag.yaml, meta.tag.metadata, meta.tag.preprocessor",
            "settings": {
                "foreground": "#E15A60"
            }
        },
        {
            "name": "Soft yellow",
            "scope": "entity.name.type.class, support.type.python, support.type.property-name.json, meta.tag.metadata.script",
            "settings": {
                "foreground": "#fdd481"
            }
        },
        {
            "name": "Parent class",
            "scope": "meta.class.inheritance",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Soft orange",
            "scope": "constant.language, constant.other.caps.python, variable.other.constant, constant.numeric",
            "settings": {
                "foreground": "#f19f73"
            }
        },
        {
            "name": "Keyword, tags",
            "scope": "keyword, storage.type, storage.modifier, meta.tag.structure, meta.tag.xml",
            "settings": {
                "foreground": "#ca84ca"
            }
        },
        {
            "name": "Punctuation, operators",
            "scope": "punctuation, keyword.operator, entity.other.attribute-name, meta.brace.round, meta.brace.square",
            "settings": {
                "foreground": "#5FB3B3"
            }
        },
        {
            "name": "String",
            "scope": "string, constant.character",
            "settings": {
                "foreground": "#99C794"
            }
        },
        {
            "name": "Decorators",
            "scope": "meta.function.decorator",
            "settings": {
                "foreground": "#6699CC",
            }
        },
        {
            "name": "Function",
            "scope": "meta.function-call.python, entity.name.function, support.function, support.class",
            "settings": {
                "foreground": "#6699CC"
            }
        },
        {
            "name": "Function parameters",
            "scope": "variable.parameter.function.language.python, variable.parameter.function-call.python, variable.parameter.js",
            "settings": {
                "foreground": "#b3cce6",
                "fontStyle": "italic",
            }
        },
        {
            "name": "Debug",
            "scope": "log.debug, token.debug-token",
            "settings": {
                "foreground": "#8e52b9"
            }
        },
        {
            "name": "Info",
            "scope": "log.info, token.info-token",
            "settings": {
                "foreground": "#99C794"
            }
        },
        {
            "name": "Warning",
            "scope": "log.warning, token.warn-token",
            "settings": {
                "foreground": "#fdd481"
            }
        },
        {
            "name": "Error",
            "scope": "log.error, token.error-token",
            "settings": {
                "foreground": "#E15A60"
            }
        },
        {
            "name": "URL",
            "scope": "*url*, *link*, *uri*",
            "settings": {
                "fontStyle": "underline"
            }
        }
    ]
}